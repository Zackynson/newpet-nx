{
	"name": "newpet",
	"version": "0.1.0",
	"license": "MIT",
	"scripts": {
		"prepare-husky": "husky install",
		"prepare-gitleaks": "docker pull zricethezav/gitleaks:latest",
		"prepare": "npm run prepare-husky && npm run prepare-gitleaks",
		"lint": "npx nx affected:lint --uncommitted --parallel",
		"lint:fix": "npm run lint -- --fix && git diff --name-only --cached | xargs -l git add",
		"pretty": "pretty-quick --staged --ignore-path .eslintignore",
		"docs": "typedoc",
		"leaks:protect": "gitleaks protect --staged --verbose || docker run --rm -v $(pwd):/repo zricethezav/gitleaks:latest protect --source='/repo' --staged --verbose",
		"leaks:protect:report": "gitleaks protect --staged -r $(pwd)/gitleaks-report.json || docker run --rm -v $(pwd):/repo zricethezav/gitleaks:latest protect --source='/repo' -v --staged",
		"install-deps:all": "nx run-many --target=install-dependencies --all",
		"build:all": "nx run-many --target=build --configuration=production --all",
		"build:affected": "nx affected --target=build --configuration=production",
		"install-deps:affected": "nx affected --target=install-dependencies",
		"build:affected:dev": "npm run build:affected -- --base=dev --head=HEAD && npm install-deps:affected -- --base=dev --head=HEAD",
		"build:affected:beta": "npm run build:affected -- --base=beta --head=HEAD && npm install-deps:affected -- --base=beta --head=HEAD",
		"build:affected:prod": "npm run build:affected -- --base=master --head=HEAD && npm install-deps:affected -- --base=master --head=HEAD",
		"register-deploy:affected": "nx affected --target=register-deploy",
		"register-deploy:all": "./scripts/register-all-deploy.sh",
		"deploy": "cdk deploy --require-approval never --all",
		"build-and-deploy": " npx nx run-many --target=build --configuration=production --all && npx nx run-many --target=install-dependencies --all=true && cdk deploy"
	},
	"private": true,
